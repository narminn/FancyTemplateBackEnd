<!--================Creative Feature Area =================-->
<section class="creative_feature_area">
    <div class="container">
        <div class="c_feature_box">
            <div class="row">
                @foreach (var item in Model._feature)
                {
                    <div class="col-lg-4">
                        <div class="c_box_item">
                            <a href="@item.feature_boxes_url"><h4><i class="@item.feature_boxes_icon" aria-hidden="true" style="margin-right:10px"></i>@item.feature_boxes_title</h4></a>
                            <p>@item.feature_boxes_content </p>
                        </div>
                    </div>
                }



            </div>
        </div>
        <div class="digital_feature p_100">
            <div class="row">
                <div class="col-lg-6">
                    <div class="d_feature_text">
                        <div class="main_title">
                            <h2>We Are A Creative <br /> Digital Agency Focused on Growing Brands Online</h2>
                        </div>
                        <p>Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderi.</p>
                        <p>Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia dese mollit anim id est laborum. Sed ut perspiciatis unde omnis iste.</p>
                        <a class="read_btn" href="#">Read more</a>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="d_feature_img">
                        <img src="~/Assets/img/feature-right.jpg" alt="">
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--================End Creative Feature Area =================-->
<!--================Industries Area =================-->
<section class="industries_area">
    @foreach (var item in Model._industry)
    {


        <div class="left_indus">
            <div class="indus_img">
                <img src="~/Uploads/@item.industries_img" alt="">
            </div>
        </div>
        <div class="right_indus">
            <div class="indus_text">
                <div class="main_title">
                    <h2>@item.industries_title</h2>
                    <p>@item.industries_content</p>
                </div>
                <div class="our_skill_inner">
                    <div class="single_skill">
                        <h3>Website Design</h3>
                        <div class="progress" data-value="90">
                            <div class="progress-bar">
                                <div class="progress_parcent"><span class="counter">90</span>%</div>
                            </div>
                        </div>
                    </div>
                    <div class="single_skill">
                        <h3>Brand Strategy</h3>
                        <div class="progress" data-value="95">
                            <div class="progress-bar">
                                <div class="progress_parcent"><span class="counter">95</span>%</div>
                            </div>
                        </div>
                    </div>
                    <div class="single_skill">
                        <h3>Digital Marketing</h3>
                        <div class="progress" data-value="85">
                            <div class="progress-bar">
                                <div class="progress_parcent"><span class="counter">85</span>%</div>
                            </div>
                        </div>
                    </div>
                    <div class="single_skill">
                        <h3>Website Development</h3>
                        <div class="progress" data-value="90">
                            <div class="progress-bar">
                                <div class="progress_parcent"><span class="counter">90</span>%</div>
                            </div>
                        </div>
                    </div>
                </div>
                <a class="read_btn" href="@item.industries_url">Work with us</a>
            </div>
        </div>
    }
</section>
<!--================End Industries Area =================-->
<!--================Our Service Area =================-->
@foreach (var item in Model._service)
{
    <section class="service_area" style="background-image:url(../Uploads/@item.service_img)">
        <div class="container">



            <div class="center_title">
                <h2>@item.service_title</h2>
                <p>@item.service_content</p>
            </div>
            <div class="row service_item_inner">
                @foreach (var item_content in Model._service_content)
                {
                    <div class="col-lg-4">
                        <div class="service_item">
                            <i class="@item_content.service_contents_icon"></i>
                            <h4>@item_content.service_contents_title</h4>
                            <p>@item_content.service_contents_content</p>
                        </div>
                    </div>
                }


            </div>

        </div>
    </section>
}
<!--================End Our Service Area =================-->
<!--================Testimonials Area =================-->
<section class="testimonials_area p_100">
    <div class="container">
        <div class="testimonials_slider owl-carousel">
            @foreach (var item in Model._testimonial)
            {
                <div class="item">
                    <div class="media">
                        <img class="d-flex rounded-circle" src="~/Uploads/@item.testimonials_slider_img" alt="" style="width:150px;height:200px;">
                        <div class="media-body">
                            <i class="@item.testimonials_slider_icon"></i>
                            <p>@item.testimonials_slider_content.Substring(0, 250)...</p>
                            <h4><a href="#">@item.testimonials_slider_author</a>@item.testimonials_slider_position</h4>
                        </div>
                    </div>
                </div>
            }



        </div>
    </div>
</section>
<!--================End Testimonials Area =================-->
<!--================Project Area =================-->
<section class="project_area">
    <div class="container">
        <div class="project_inner">
            <div class="center_title">
                <h2>Ready To Discuss Your Project? </h2>
                <p>There are many ways to contact us. You may drop us a line, give us a call or send an email, choose what suits you the most.</p>
            </div>
            <a class="tp_btn" href="#">WORK WITH US</a>
        </div>
    </div>
</section>
<!--================End Project Area =================-->
<!--================Latest News Area =================-->
<section class="latest_news_area p_100">
    <div class="container">
        <div class="b_center_title">
            <h2>Latest News</h2>
            <p>We Are A Creative Digital Agency. Focused on Growing Brands Online</p>
        </div>
        <div class="l_news_inner">
            <div class="row">
                @foreach (var item in Model._blog)
                {
                    <div class="col-lg-4 col-md-6">
                        <div class="l_news_item">
                            <div class="l_news_img"><a href="#"><img class="img-fluid" src="~/Uploads/@item.blog_img" alt=""></a></div>
                            <div class="l_news_content">
                                <a href="#"><h4>@item.blog_title</h4></a>
                                <p>@item.blog_content.Substring(0, 200)...</p>
                                <a class="more_btn" href="#">Learn More</a>
                            </div>
                        </div>
                    </div>
                }


            </div>
        </div>
    </div>
</section>
<!--================End Latest News Area =================-->
